{
  "name": "while-distracted",
  "description": "TechNinja's gatsby blog experiment",
  "version": "0.5.0",
  "author": "James T <james@tn42.com>",
  "repository": "github:techninja/while-distracted",
  "dependencies": {
    "gatsby": "^1.9.247",
    "gatsby-image": "^1.0.51",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-catch-links": "^1.0.22",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "gatsby-plugin-sass": "^1.0.26",
    "gatsby-plugin-sharp": "^1.6.44",
    "gatsby-remark-generic-extensions": "0.0.1",
    "gatsby-remark-images": "^1.5.65",
    "gatsby-remark-responsive-iframe": "^1.4.19",
    "gatsby-source-filesystem": "^1.5.35",
    "gatsby-transformer-remark": "^1.7.40",
    "gatsby-transformer-sharp": "^1.6.24",
    "gatsby-transformer-yaml": "^1.5.17",
    "lodash": "^4.17.10",
    "react-helmet": "^5.2.0",
    "react-media": "^1.8.0"
  },
  "keywords": [
    "gatsby"
  ],
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier"
    ],
    "env": {
      "es6": true,
      "browser": true
    },
    "globals": {
      "expect": true
    },
    "rules": {
      "no-plusplus": 0,
      "semi": 2,
      "before": true,
      "after": true,
      "no-multi-spaces": "error",
      "space-in-parens": "error",
      "space-before-blocks": "error"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -p 8001",
    "format": "prettier --write 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "gh-pages": "^1.2.0",
    "prettier": "^1.12.0"
  }
}
